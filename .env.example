# Extraction API Environment Variables
# Copy this to .env for local development
# Add these as secrets in Railway for production

# Cohere API Key (Command A Vision)
COHERE_API_KEY=your_cohere_api_key_here

# Roboflow API Key
ROBOFLOW_API_KEY=your_roboflow_api_key_here

# Roboflow Model (format: project-name/version)
ROBOFLOW_MODEL=architectural-detail-detection/1

# Supabase (optional, for direct database access)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_service_key

# Port (Railway sets this automatically)
PORT=5050
